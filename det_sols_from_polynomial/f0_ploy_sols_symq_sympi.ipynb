   "source": [
    "pi = 0.00001\n",
    "q = 10\n",
    "r = 1/q\n",
    "l = 0.05\n",
    "\n",
    "# coefs second degree polynomial: ax**2 + bx + c = 0\n",
    "a = -l\n",
    "b = 2*pi*(1-l)+l+r\n",
    "c = -r\n",
    "\n",
    "# folution to f0:\n",
    "root = np.sqrt(b**2-4*l*r)\n",
    "f0_1 = (-b+root)/(2*a)\n",
    "f0_2= (-b-root)/(2*a)\n",
    "print(f0_1)\n",
    "print(f0_2)"
   ]
